From 333b345d9ffa23816b4a35369d5946c6b25b0cc3 Mon Sep 17 00:00:00 2001
From: LuK1337 <priv.luk@gmail.com>
Date: Tue, 10 Feb 2015 22:52:13 +0100
Subject: [PATCH] core: arm: Compile with 4.8

Change-Id: Ib16f6ce99a57ef773a145df90d76e805c0cc07e9
---
 core/combo/TARGET_linux-arm.mk | 8 +++++---
 1 file changed, 5 insertions(+), 3 deletions(-)

diff --git a/core/combo/TARGET_linux-arm.mk b/core/combo/TARGET_linux-arm.mk
index 3497662..d400c7a 100644
--- a/core/combo/TARGET_linux-arm.mk
+++ b/core/combo/TARGET_linux-arm.mk
@@ -35,7 +35,7 @@ TARGET_ARCH_VARIANT := armv5te
 endif
 
 ifeq ($(strip $(TARGET_GCC_VERSION_EXP)),)
-TARGET_GCC_VERSION := 4.7
+TARGET_GCC_VERSION := 4.8
 else
 TARGET_GCC_VERSION := $(TARGET_GCC_VERSION_EXP)
 endif
@@ -121,9 +121,11 @@ TARGET_GLOBAL_CFLAGS += \
 # We cannot turn it off blindly since the option is not available
 # in gcc-4.4.x.  We also want to disable sincos optimization globally
 # by turning off the builtin sin function.
-ifneq ($(filter 4.6 4.6.% 4.7 4.7.%, $(TARGET_GCC_VERSION)),)
+ifneq ($(filter 4.6 4.6.% 4.7 4.7.% 4.8 4.8.% 4.9 4.9.%, $(TARGET_GCC_VERSION)),)
 TARGET_GLOBAL_CFLAGS += -Wno-unused-but-set-variable -fno-builtin-sin \
-			-fno-strict-volatile-bitfields
+                        -fno-strict-volatile-bitfields \
+                        -Wno-error=unused-parameter \
+                        -Wno-error=unused-variable
 endif
 
 # This is to avoid the dreaded warning compiler message:
-- 
2.1.4

